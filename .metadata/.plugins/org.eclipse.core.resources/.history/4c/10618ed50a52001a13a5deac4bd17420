import java.util.*;
public class ab{
	
	public static void main(String[] args) {
		
		List<Integer> input = new ArrayList<Integer>();
		input.add(1);
		input.add(1);
		input.add(3);
		input.add(4);
		System.out.println(ab.cutBamboo(input));
			
	}
	
	static public List<Integer> cutBamboo(List<Integer> lengths){
		List<Integer> result = new ArrayList<Integer>();
		for(int i = 0; i < lengths.size(); i++) {
			result.add(lengths.size());
			int shortBamboo = lengths.get(0);
			for(int j = 1; j < lengths.size(); j++) {
				if ( shortBamboo > lengths.get(j) ) {
					shortBamboo = lengths.get(j);
				}
			}
			System.out.println("Short " +  shortBamboo);
			for(int k = 0; k < lengths.size(); k++) {
				int temp = lengths.get(k) - shortBamboo;
				System.out.println("temp " + k + " " + temp);
				if(temp == 0) {
					System.out.println(k);
					lengths.remove(k);
					k--;
				} else {
					lengths.add(k, temp);
				}
			}
			System.out.println(lengths.toString());
			return result;
			
		}
		return result;
		
	}
	
}
