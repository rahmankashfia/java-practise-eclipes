package apex.advance.java;

public class IsPermutation {
	public static void main(String[] args) {
		String a = "abcdef";
		String b = "fedcbq";
		if(a == null || b == null || a.length() != b.length()) {
			//return "No Matching";
		} else {
			for (int i = 0; i < a.length(); i++) {
				if (b.indexOf(a.charAt(i)) >= 0) {
					continue;
				} else {
					//return "No Matching";
				}
			}
			
		}
		return "Maching found";		
		
	}

}
