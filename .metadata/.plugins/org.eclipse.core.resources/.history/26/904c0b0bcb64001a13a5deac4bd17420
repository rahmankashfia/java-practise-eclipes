package apex.advance.java;

public final class ImmutableClass {
	final String string;
	
	public ImmutableClass(){
		this.string = "Kashfia";
	}
	public ImmutableClass(String string) {
		this.string = string;
	}
	
	public String getString() {
		return this.string; 
	}
	
	public static void main(String[] args) {
		ImmutableClass immutableClass = new ImmutableClass("sweta");
		System.out.println(immutableClass.getString());
		
	
	}
	
}
