public static int getMinimumMoves(List<Integer> arr) {
    // Write your code here
    for(int i = 0; i < arr.size(); i++){
        System.out.println(get_max(arr, arr.size() - i));
    }
    }


public static int get_max(List<Integer> arr, int lastInd) {
    int max = 0;
    for(int i  = 0; i < lastInd; i++) {
        if (max < arr.get(i)) {
            max = arr.get(i);
    }
    return max;
}
}