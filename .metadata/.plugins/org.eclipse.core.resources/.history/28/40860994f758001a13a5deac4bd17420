	int[] b = {2,4,6};
	int[] c = new int[a.length + b.length];
	int i = 0;
	int j = 0;
	int k = 0; 
	while(true) {
		if (i == a.length || i == b.length) {
			break;
		}
		else {
			if(a[i] > b[j]) {
				c[i] = b[j];
				j++;
			} else {
				
			}
			
		}
	}
	
	
	
}

}
